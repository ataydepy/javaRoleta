package s;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Random;


public class Index{
	public static void main(String[]args) {
	
		Random random = new Random();
		
		System.out.println("Welcome to the game");
		
		ArrayList<String> nomes = new ArrayList<>(Arrays.asList("Ana", "Allan", "Carlos", "Clarice","Clarysse","Davi","Samuel","Gabriel","Thiago","Raul","Nathan","Lucas","Paula","Nicolas","Mariany","Sarah","Emanuelly","Pedro"));
		
		int randomIndex = random.nextInt(nomes.size());
		String nomeEscolhido = nomes.get(randomIndex);
		
		System.out.println(nomeEscolhido + " will die");
		
		}
	
	}
	
